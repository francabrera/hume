{
  "_meta": {
    "sources": ["loopback/common/models", "loopback/server/models", "./models"],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "db",
    "public": false
  },
  "AccessToken": {
    "dataSource": "db",
    "public": false
  },
  "ACL": {
    "dataSource": "db",
    "public": false
  },
  "user": {
    "dataSource": "db",
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "login": true,
          "logout": true,
          "replaceOrCreate": true
        }
      }
    }
  }
}
